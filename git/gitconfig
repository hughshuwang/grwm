[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Shu Wang
	email = hugh.shu.wang@gmail.com
[color]
	ui = true
[alias]
	lg = log --color --graph \n    --pretty=format:'%Cred%h%Creset \n    -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' \n    --abbrev-commit
